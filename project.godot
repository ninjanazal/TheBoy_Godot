; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Concept1"
config/description="Party Voxel Game "
run/main_scene="res://logic/scenes/game_scenes/intro/intro_scene.tscn"
boot_splash/image="res://assets/graphics/usefull/transparent_128_128.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.0666667, 0.0666667, 0.0705882, 1 )
config/icon="res://assets/graphics/logos/voxelLogo.png"

[autoload]

Application="*res://src/Application/Application.cs"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=960
window/size/test_height=540
window/dpi/allow_hidpi=true
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/snap_controls_to_pixels=false

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true
2d/physics_engine="GodotPhysics"
3d/physics_engine="GodotPhysics"

[rendering]

quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=3
vram_compression/import_etc=true
quality/directional_shadow/size=2048
quality/shadows/filter_mode.mobile=1
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/shading/force_vertex_shading=true
quality/shading/force_lambert_over_burley=true
quality/shading/force_blinn_over_ggx=true
quality/shading/use_physical_light_attenuation=true
gles2/compatibility/disable_half_float=true
environment/default_clear_color=Color( 0.0784314, 0.0784314, 0.0784314, 1 )
quality/filters/use_fxaa=true
environment/default_environment="res://default_env.tres"
quality/dynamic_fonts/use_oversampling=false
