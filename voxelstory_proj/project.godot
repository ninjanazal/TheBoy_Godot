[application]

config/name="Voxel Party"
config/description="Party Voxel Game "
run/main_scene="res://logic/scenes/game_scenes/intro/intro_scene.tscn"
boot_splash/image="res://assets/graphics/usefull/transparent_128_128.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.0666667, 0.0666667, 0.0705882, 1 )
config/icon="res://assets/graphics/logos/voxelLogo.png"

[autoload]

Application="*res://src/Application/application.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=960
window/size/test_height=540
window/dpi/allow_hidpi=true
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/snap_controls_to_pixels=false

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true
2d/physics_engine="GodotPhysics"
3d/physics_engine="GodotPhysics"

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
quality/filters/anisotropic_filter_level=2
gles2/compatibility/disable_half_float=true
environment/default_clear_color=Color( 0.156863, 0.156863, 0.156863, 1 )
environment/default_environment="res://default_env.tres"
